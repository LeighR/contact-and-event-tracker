<h1>Editing contact group</h1>

<% form_for(@contact_group) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit 'Save Changes' %>
<% end %>

<p class="admin_links">
  <%= link_to 'List Users in this Group', @contact_group %> |
  <%= link_to 'List of Groups', contact_groups_path %>
</p>